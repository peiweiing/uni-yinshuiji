
.view{
	min-height: calc(100vh);
}
.bgcimg{
	min-height: calc(50vh);
	background-color: #1789E1;
}
.bgcimg .topimg{
	color: #000000;
	width: 90%;
	height: calc(60vh);
	background: url(data:image/png;base64,) no-repeat;
	background-size: 100% 100%;
	margin-top: 10%;
	padding: 4% 2%;
	box-sizing: border-box;
	font-size: 16px;
}
.btn{
	min-height: calc(50vh);
	margin-top: 20%;
}
.btn .bgcColor{
	width: 90%;
	border-radius: 80rpx;
	font-size: 16px;
}

