
.bgcColor{
	min-height: calc(100vh);
}
.topimg{
	height: calc(60vh);
	background: url(data:image/png;base64,) no-repeat;
	background-size: 200% 80%;
	-webkit-animation: topimg 15s infinite linear;
	        animation: topimg 15s infinite linear;
}
.topimg .top{
	height: calc(40vh);
	padding-top: 5%;
}
.topimg .top image{
	width: 220rpx;height: 380rpx;
}
.topimg .cent{
	color: #1789E0;
	background-color: #fff;
	margin: 0 10% 10%;
	padding: 4%;
	border-radius: 20rpx;
}
.topimg .cent-top{
	margin-bottom: 2%;
}
.topimg .cent .cent-top image{
	width: 50rpx;height: 50rpx;
}
.topimg .end{
	font-size: 12px;
	width: 55%;
	color: #fff;
	background-color: #1789E0;
	padding: 0;
	border-radius: 80rpx;
}
.topimg .end:hover,.topimg .end:target{
	-webkit-transition: .5s;
	transition: .5s;
	background-color: #1aa0ff;
}
.endimg{
	height: calc(40vh);
	padding: 10% 0 0;
	box-sizing: border-box;
}
.endimg .endcs{
	font-size: 14px;
	width: 240rpx;
	height: 110rpx;
	border-radius: 80rpx;
}
.endimg .endcs:hover,.endimg .endcs:target{
	-webkit-transform: scale(.8,.8);
	        transform: scale(.8,.8);
	-webkit-transition: 1s;
	transition: 1s;
	box-shadow: 0 0 40rpx #FF834C;
}
.endimg view:first-child{
	color: #000;
	border: 1px solid #FF834C;
}
.endimg view:last-child{
	color: #FFF;
	background-color: #FF834C;
}

